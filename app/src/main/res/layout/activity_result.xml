<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F18C42">

    <ScrollView
        android:id="@+id/scrollView2"
        android:layout_width="match_parent"
        android:layout_height="590dp"
        android:layout_marginTop="141dp"
        android:background="@drawable/white_design_shape"
        android:fillViewport="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <TextView
                android:id="@+id/category_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="70dp"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="300dp"
                android:fontFamily="@font/inter"
                android:text="Categoria:"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.666"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/right_ans_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="70dp"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="237dp"
                android:fontFamily="@font/inter"
                android:text="Raspunsuri corecte:"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/category_label" />

            <TextView
                android:id="@+id/wrong_ans_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="70dp"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="241dp"
                android:fontFamily="@font/inter"
                android:text="Raspunsuri gresite:"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/right_ans_label" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="50dp"
                android:layout_marginTop="32dp"
                android:layout_marginEnd="300dp"
                android:text="TextView"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="parent" />

            <TextView
                android:id="@+id/percentage_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="70dp"
                android:layout_marginTop="40dp"
                android:layout_marginEnd="247dp"
                android:fontFamily="@font/inter"
                android:text="Procentaj corecte:"
                android:textSize="16sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/wrong_ans_label" />

            <TextView
                android:id="@+id/date_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="48dp"
                android:layout_marginTop="40dp"
                android:fontFamily="@font/inter"
                android:text="Data:"
                android:textSize="16sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/percentage_label" />

            <TextView
                android:id="@+id/result_label"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="48dp"
                android:layout_marginTop="40dp"
                android:fontFamily="@font/inter"
                android:text="Rezultat:"
                android:textSize="16sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/date_label" />

            <TextView
                android:id="@+id/category_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="32dp"
                android:fontFamily="@font/inter"
                android:text="category_placeholder"
                android:textColor="#F18C42"
                android:textSize="16sp"
                app:layout_constraintStart_toEndOf="@+id/category_label"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/right_ans_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="40dp"
                android:fontFamily="@font/inter"
                android:text="corecte_placeholder"
                android:textColor="#F18C42"
                android:textSize="16sp"
                app:layout_constraintStart_toEndOf="@+id/right_ans_label"
                app:layout_constraintTop_toBottomOf="@+id/category_value" />

            <TextView
                android:id="@+id/wrong_ans_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="40dp"
                android:fontFamily="@font/inter"
                android:text="gresite_placeholder"
                android:textColor="#F18C42"
                android:textSize="16sp"
                app:layout_constraintStart_toEndOf="@+id/wrong_ans_label"
                app:layout_constraintTop_toBottomOf="@+id/right_ans_value" />

            <TextView
                android:id="@+id/percentage_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="4dp"
                android:layout_marginTop="40dp"
                android:fontFamily="@font/inter"
                android:text="procentaj_placeholder"
                android:textColor="#F18C42"
                android:textSize="16sp"
                app:layout_constraintStart_toEndOf="@+id/percentage_label"
                app:layout_constraintTop_toBottomOf="@+id/wrong_ans_label" />

            <TextView
                android:id="@+id/result_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="40dp"
                android:fontFamily="@font/inter"
                android:text="rezultat_placeholder"
                android:textColor="#F18C42"
                android:textSize="16sp"
                app:layout_constraintStart_toEndOf="@+id/result_label"
                app:layout_constraintTop_toBottomOf="@+id/date_value" />

            <TextView
                android:id="@+id/date_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_marginTop="40dp"
                android:text="01.01.1970"
                android:textColor="#F18C42"
                android:textSize="16sp"
                app:layout_constraintStart_toEndOf="@+id/date_label"
                app:layout_constraintTop_toBottomOf="@+id/percentage_label" />

            <Button
                android:id="@+id/show_my_ans"
                android:layout_width="301dp"
                android:layout_height="66dp"
                android:layout_marginTop="50dp"
                android:layout_marginBottom="16dp"
                android:backgroundTint="#F18C42"
                android:fontFamily="@font/inter"
                android:text="Arata-mi raspunsurile"
                android:textColor="#000000"
                android:textSize="20sp"
                app:layout_constraintBottom_toTopOf="@+id/back_to_menu"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/result_value" />

            <Button
                android:id="@+id/back_to_menu"
                android:layout_width="301dp"
                android:layout_height="66dp"
                android:layout_marginBottom="32dp"
                android:backgroundTint="#F18C42"
                android:fontFamily="@font/inter"
                android:text="Du-ma acasa"
                android:textColor="#000000"
                android:textSize="20sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/inter"
        android:text="Rezultat final"
        android:textSize="34sp"
        app:layout_constraintBottom_toTopOf="@+id/scrollView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>